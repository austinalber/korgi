    "@pusher/chatkit-client": "^1.13.2",
    "@pusher/chatkit-client-react": "^0.1.2",
    "@pusher/chatkit-server": "^2.1.1",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chatkit": "0.0.1-security",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
